{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"author": "",
	"license": "MIT",
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.json",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "ts-node -r tsconfig-paths/register src/main.ts",
		"start:dev": "cross-env NODE_ENV=development tsc-watch -p tsconfig.json --onSuccess \"node dist/main.js\"",
		"start:debug": "cross-env NODE_ENV=development tsc-watch -p tsconfig.json --onSuccess \"node --inspect-brk dist/main.js\"",
		"start:prod": "node dist/main.js",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/common": "^7.0.13",
		"@nestjs/config": "0.4.2",
		"@nestjs/core": "^7.0.13",
		"@nestjs/mongoose": "^7.0.1",
		"@nestjs/platform-express": "^7.0.13",
		"@nestjs/schedule": "^0.4.0",
		"@types/config": "0.0.36",
		"@types/cron": "^1.7.2",
		"@types/elementtree": "^0.1.0",
		"@types/jszip": "^3.4.1",
		"@types/mongoose": "^5.7.20",
		"@types/tough-cookie": "^4.0.0",
		"@types/winston": "^2.4.4",
		"config": "^3.3.1",
		"cross-env": "^7.0.2",
		"form-data": "^3.0.0",
		"got": "^11.1.4",
		"moment": "^2.25.3",
		"mongoose": "^5.9.14",
		"nest-schedule": "^0.6.4",
		"nest-winston": "^1.3.4",
		"nestjs-rate-limiter": "^1.0.1",
		"nestjs-telegraf": "^0.7.3",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^6.5.5",
		"telegraf": "^3.35.0",
		"tough-cookie": "^4.0.0",
		"winston": "^3.2.1",
		"xlsx-template": "^0.5.0"
	},
	"devDependencies": {
		"@nestjs/testing": "^7.0.13",
		"@types/express": "4.17.6",
		"@types/jest": "25.2.2",
		"@types/node": "14.0.1",
		"@types/supertest": "2.0.9",
		"jest": "26.0.1",
		"prettier": "2.0.5",
		"supertest": "4.0.2",
		"ts-jest": "^26.0.0",
		"ts-node": "8.10.1",
		"tsc-watch": "^4.2.5",
		"tsconfig-paths": "3.9.0",
		"tslint": "6.1.2",
		"typescript": "3.9.2"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "./coverage",
		"testEnvironment": "node"
	}
}
