{
	"name": "backend",
	"version": "2.0.0",
	"description": "",
	"author": "Eldar Salimzebarov",
	"license": "FSF",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"post-commit": "git update-index -g"
		}
	},
	"lint-staged": {
		"*.{js.ts}": [
			"prettier --write",
			"git add"
		]
	},
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.json",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "ts-node -r tsconfig-paths/register src/main.ts",
		"start:dev": "cross-env NODE_ENV=development tsc-watch -p tsconfig.json --onSuccess \"node dist/main.js\"",
		"start:debug": "cross-env NODE_ENV=development tsc-watch -p tsconfig.json --onSuccess \"node --inspect-brk dist/main.js\"",
		"start:prod": "node dist/main.js",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/common": "^7.3.2",
		"@nestjs/config": "0.5.0",
		"@nestjs/core": "^7.3.2",
		"@nestjs/mongoose": "^7.0.2",
		"@nestjs/platform-express": "^7.3.2",
		"@nestjs/schedule": "^0.4.0",
		"@types/body-parser": "^1.19.0",
		"@types/config": "0.0.36",
		"@types/cron": "^1.7.2",
		"@types/elementtree": "^0.1.0",
		"@types/jszip": "^3.4.1",
		"@types/mongoose": "^5.7.31",
		"@types/tough-cookie": "^4.0.0",
		"@types/winston": "^2.4.4",
		"body-parser": "^1.19.0",
		"config": "^3.3.1",
		"cross-env": "^7.0.2",
		"form-data": "^3.0.0",
		"got": "^11.5.0",
		"moment": "^2.27.0",
		"mongoose": "^5.9.23",
		"nest-schedule": "^0.6.4",
		"nest-winston": "^1.3.5",
		"nestjs-telegraf": "^1.2.1",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^6.6.0",
		"tough-cookie": "^4.0.0",
		"winston": "^3.3.3",
		"xlsx-template": "^1.2.0"
	},
	"devDependencies": {
		"@nestjs/testing": "^7.3.2",
		"@types/jest": "26.0.4",
		"@types/node": "14.0.22",
		"@types/rimraf": "^3.0.0",
		"@types/supertest": "2.0.10",
		"@types/tsc-watch": "^4.2.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"jest": "26.1.0",
		"prettier": "2.0.5",
		"supertest": "4.0.2",
		"ts-jest": "^26.1.1",
		"ts-node": "8.10.2",
		"tsc-watch": "^4.2.9",
		"tsconfig-paths": "3.9.0",
		"tslint": "6.1.2",
		"typescript": "3.9.6"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "./coverage",
		"testEnvironment": "node"
	}
}
