{
	"name": "backend",
	"version": "2.0.0",
	"description": "",
	"author": "Eldar Salimzebarov",
	"license": "MIT",
	"private": true,
	"lint-staged": {
		"*.js.ts": [
			"npm run format",
			"git add"
		]
	},
	"scripts": {
		"prepare": "cd .. && husky install backend/.husky",
		"build": "rimraf dist && tsc -d false -p tsconfig.json",
		"start": "cross-env NODE_ENV=production npm run run:js",
		"start:ts": "cross-env NODE_OPTIONS=--max-old-space-size=4096 ts-node -r tsconfig-paths/register src/main.ts",
		"start:dev": "cross-env NODE_ENV=development NODE_OPTIONS=--max-old-space-size=4096 tsc-watch -p tsconfig.json --onSuccess \"node dist/main.js\"",
		"start:prod": "cross-env NODE_ENV=development npm run run:js",
		"run:js": "cross-env NODE_OPTIONS=--max-old-space-size=4096 node dist/main.js",
		"pm2:dev": "pm2 start ../process.yml --only rate-cluster-dev",
		"pm2:list": "pm2 list",
		"pm2:stop": "pm2 stop rate-cluster-dev && pm2 delete rate-cluster-dev",
		"pm2:monit ": "pm2 monit",
		"pm2:info ": "pm2 info rate-cluster-dev",
		"pm2:logs ": "pm2 logs",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"format": "prettier --write --config ./.prettierrc --check \"src/**/*.ts\""
	},
	"dependencies": {
		"@nestjs/common": "^8.4.7",
		"@nestjs/config": "^2.1.0",
		"@nestjs/core": "^8.4.7",
		"@nestjs/mongoose": "9.1.1",
		"@nestjs/platform-express": "^8.4.7",
		"@nestjs/schedule": "^2.0.1",
		"@types/body-parser": "^1.19.2",
		"@types/config": "^0.0.41",
		"@types/cron": "^1.7.3",
		"@types/elementtree": "^0.1.1",
		"@types/jszip": "^3.4.1",
		"@types/tough-cookie": "^4.0.2",
		"@types/winston": "^2.4.4",
		"body-parser": "^1.20.0",
		"config": "^3.3.7",
		"cross-env": "^7.0.3",
		"form-data": "^4.0.0",
		"got": "^11.8.1",
		"moment": "^2.29.3",
		"mongoose": "6.4.0",
		"nest-schedule": "^0.6.4",
		"nestjs-telegraf": "^2.4.1",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.5.5",
		"telegraf": "^4.8.5",
		"tough-cookie": "^4.0.0",
		"typescript": "4.7.4",
		"winston": "^3.7.2",
		"xlsx-template": "1.0.0"
	},
	"devDependencies": {
		"@types/tsc-watch": "^4.2.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.0.2",
		"pm2": "^5.2.0",
		"prettier": "^2.7.1",
		"ts-node": "^10.8.1",
		"tsc-watch": "^5.0.3",
		"tsconfig-paths": "^4.0.0",
		"tslint": "^6.1.3"
	}
}
