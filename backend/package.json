{
	"name": "backend",
	"version": "2.0.0",
	"description": "",
	"author": "Eldar Salimzebarov",
	"license": "FSF",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"post-commit": "git update-index -g"
		}
	},
	"lint-staged": {
		"*.{js.ts}": [
			"prettier --write",
			"git add"
		]
	},
	"scripts": {
		"build": "rimraf dist && tsc -d false -p tsconfig.json",
		"start": "cross-env NODE_ENV=production npm run run:js",
		"start:ts": "cross-env NODE_OPTIONS=--max-old-space-size=4096 ts-node -r tsconfig-paths/register src/main.ts",
		"start:dev": "cross-env NODE_ENV=development NODE_OPTIONS=--max-old-space-size=4096 tsc-watch -p tsconfig.json --onSuccess \"node dist/main.js\"",
		"start:prod": "cross-env NODE_ENV=development npm run run:js",
		"run:js": "cross-env NODE_OPTIONS=--max-old-space-size=4096 node dist/main.js",
		"pm2:dev": "pm2 start ../process.yml --only rate-cluster-dev",
		"pm2:list": "pm2 list",
		"pm2:stop": "pm2 stop rate-cluster-dev && pm2 delete rate-cluster-dev",
		"pm2:monit ": "pm2 monit",
		"pm2:info ": "pm2 info rate-cluster-dev",
		"pm2:logs ": "pm2 logs",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"format": "prettier --write \"src/**/*.ts\"",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/bull": "^0.3.1",
		"@nestjs/common": "^7.6.5",
		"@nestjs/config": "^0.5.0",
		"@nestjs/core": "^7.6.5",
		"@nestjs/mongoose": "7.0.2",
		"@nestjs/platform-express": "^7.6.5",
		"@nestjs/schedule": "^0.4.1",
		"@types/body-parser": "^1.19.0",
		"@types/bull": "^3.14.4",
		"@types/config": "^0.0.37",
		"@types/cron": "^1.7.2",
		"@types/elementtree": "^0.1.0",
		"@types/jszip": "^3.4.1",
		"@types/mongoose": "5.10.3",
		"@types/tough-cookie": "^4.0.0",
		"@types/winston": "^2.4.4",
		"body-parser": "^1.19.0",
		"bull": "^3.20.0",
		"config": "^3.3.3",
		"cross-env": "^7.0.3",
		"form-data": "^3.0.0",
		"got": "^11.8.1",
		"moment": "^2.29.1",
		"mongoose": "5.10.3",
		"nest-schedule": "^0.6.4",
		"nest-winston": "^1.4.0",
		"nestjs-telegraf": "^1.2.1",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^6.6.3",
		"tough-cookie": "^4.0.0",
		"winston": "^3.3.3",
		"xlsx-template": "1.0.0",
		"typescript": "4.1.3"
	},
	"devDependencies": {
		"@nestjs/testing": "^7.6.5",
		"@types/jest": "^26.0.19",
		"@types/supertest": "^2.0.10",
		"@types/tsc-watch": "^4.2.0",
		"husky": "^4.3.6",
		"jest": "^26.6.3",
		"pm2": "^4.5.1",
		"lint-staged": "^10.5.3",
		"prettier": "^2.2.1",
		"supertest": "^6.0.1",
		"ts-jest": "^26.4.4",
		"ts-node": "^9.1.1",
		"tsc-watch": "^4.2.9",
		"tsconfig-paths": "^3.9.0",
		"tslint": "^6.1.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "./coverage",
		"testEnvironment": "node"
	}
}
